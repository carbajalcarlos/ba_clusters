Type = {{Article}},
Language = {{English}},
Affiliation = {{Carayannis, EG (Reprint Author), George Washington Univ, Washington, DC 20052 USA.
   Carayannis, Elias G., George Washington Univ, Washington, DC 20052 USA.
   Sindakis, Stavros; Walter, Christian, Bangkok Univ, Bangkok, Thailand.}},
DOI = {{10.1007/s10961-013-9330-y}},
ISSN = {{0892-9912}},
