@article{ ISI:000407023500005,
Author = {Bohnsack, Rene and Pinkse, Jonatan},
Title = {{Value Propositions for Disruptive TECHNOLOGIES: RECONFIGURATION TACTICS
   IN THE CASE OF ELECTRIC VEHICLES}},
Journal = {{CALIFORNIA MANAGEMENT REVIEW}},
Year = {{2017}},
Volume = {{59}},
Number = {{4}},
Pages = {{79-96}},
Month = {{AUG}},
Abstract = {{Disruptive technologies tend to underperform on attributes that are
   considered as key attributes of incumbent technologies and require new
   value propositions to increase mainstream customer appeal. Yet, how do
   firms reconfigure their value proposition as a way to overcome the
   technological inferiority of disruptive technologies? This article
   conceptualizes and empirically investigates the process of value
   proposition reconfiguration. Based on evidence on the commercialization
