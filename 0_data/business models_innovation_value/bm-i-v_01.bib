@article{ ISI:000442230800006,
Author = {Petrulaitiene, Vitalija and Korba, Pia and Nenonen, Suvi and Jylha,
   Tuuli and Junnila, Scppo},
Title = {{From walls to experience - servitization of workplaces}},
Journal = {{FACILITIES}},
Year = {{2018}},
