@article{ ISI:000370003300003,
Author = {Wells, Peter},
Title = {{Economies of Scale Versus Small Is Beautiful: A Business Model Approach
   Based on Architecture, Principles and Components in the Beer Industry}},
Journal = {{ORGANIZATION \& ENVIRONMENT}},
Year = {{2016}},
Volume = {{29}},
Number = {{1, SI}},
Pages = {{36-52}},
Month = {{MAR}},
Abstract = {{This article defines business models for sustainability as contested and
   contextual, and provides a novel framework in terms of the architecture
   of the business, its principles and components for the analysis of such
   models. With this framework, a preliminary comparison using the engaged
   scholarship methodology is made between microbreweries and large
   multinational brewers. It is concluded that defining and determining
   comparative sustainability performance based on different business
   models results in ambiguities and contradictions that are not readily
   resolved, but a key determinant in the broad definition of business
   sustainability in the brewing sector is the degree of localism that the
   business model exhibits.}},
