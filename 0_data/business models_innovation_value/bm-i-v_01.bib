@article{ ISI:000350217700046,
Author = {Sivertsson, Olof and Tell, Joakim},
Title = {{Barriers to Business Model Innovation in Swedish Agriculture}},
Journal = {{SUSTAINABILITY}},
Year = {{2015}},
