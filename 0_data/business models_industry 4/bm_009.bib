@article{ ISI:000290929800019,
Author = {Aspara, Jaakko and Lamberg, Juha-Antti and Laukia, Arjo and Tikkanen,
   Henrikki},
Title = {{Strategic management of business model transformation: lessons from
   Nokia}},
