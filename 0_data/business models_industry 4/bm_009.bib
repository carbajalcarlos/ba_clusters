@article{ ISI:000295861700022,
Author = {Mason, Katy and Spring, Martin},
Title = {{The sites and practices of business models}},
Journal = {{INDUSTRIAL MARKETING MANAGEMENT}},
Year = {{2011}},
Volume = {{40}},
Number = {{6, SI}},
Pages = {{1032-1041}},
