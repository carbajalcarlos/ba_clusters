@article{ ISI:000253453200009,
Author = {Kshetri, Nir},
Title = {{Barriers to e-commerce and competitive business models in developing
   countries: A case study}},
Journal = {{ELECTRONIC COMMERCE RESEARCH AND APPLICATIONS}},
