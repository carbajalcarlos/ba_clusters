@article{ ISI:000294086500002,
Author = {Sorescu, Alina and Frambach, Ruud T. and Singh, Jagdip and Rangaswamy,
   Arvind and Bridges, Cheryl},
Title = {{Innovations in Retail Business Models}},
Journal = {{JOURNAL OF RETAILING}},
Year = {{2011}},
