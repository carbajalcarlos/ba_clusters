Research-Areas = {{Computer Science; Engineering; Telecommunications}},
Web-of-Science-Categories  = {{Computer Science, Hardware \& Architecture; Computer Science,
   Information Systems; Engineering, Electrical \& Electronic;
   Telecommunications}},
