@article{ ISI:000304796500010,
Author = {Klophaus, Richard and Conrady, Roland and Fichert, Frank},
Title = {{Low cost carriers going hybrid: Evidence from Europe}},
Journal = {{JOURNAL OF AIR TRANSPORT MANAGEMENT}},
Year = {{2012}},
Volume = {{23}},
Pages = {{54-58}},
Month = {{AUG}},
Abstract = {{This paper reports the findings of a survey on European airlines often
   categorized as low-cost carriers to see to which extent they have
   changed their business model towards a hybrid strategy, adopting
   features of full service network airlines. Data is provided on relative
   frequencies of attributes of the low-cost business model retained and
   modified respectively. The survey concludes that short-haul airline
   business models in Europe converge. A large percentage of low-cost
   carriers has evolved into hybrid carriers which blend low-cost traits
   with those of full service network carriers. (C) 2012 Elsevier Ltd. All
   rights reserved.}},
