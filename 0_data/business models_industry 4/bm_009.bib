@article{ ISI:000256868000003,
Author = {Voss, Chris and Roth, Aleda V. and Chase, Richard B.},
Title = {{Experience, service operations strategy, and services as destinations:
   Foundations and exploratory investigation}},
Journal = {{PRODUCTION AND OPERATIONS MANAGEMENT}},
Year = {{2008}},
Volume = {{17}},
