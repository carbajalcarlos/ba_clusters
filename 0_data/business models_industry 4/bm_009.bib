@article{ ISI:000259861700018,
Author = {Hwang, Jason and Christensen, Clayton M.},
Title = {{Disruptive Innovation In Health Care Delivery: A Framework For
   Business-Model Innovation}},
Journal = {{HEALTH AFFAIRS}},
Year = {{2008}},
Volume = {{27}},
Number = {{5}},
Pages = {{1329-1335}},
Month = {{SEP-OCT}},
Abstract = {{Disruptive innovation has brought affordability and convenience to
   customers in a variety of industries. However, health care remains
   expensive and inaccessible to many because of the lack of business-model
   innovation. This paper explains the theory of disruptive innovation and
