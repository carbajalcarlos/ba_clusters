@article{ ISI:000267541800005,
Author = {Fay, Scott},
Title = {{Competitive reasons for the Name-Your-Own-Price channel}},
Journal = {{MARKETING LETTERS}},
Year = {{2009}},
Volume = {{20}},
Number = {{3}},
Pages = {{277-293}},
Month = {{SEP}},
Abstract = {{This paper shows that the Name-Your-Own-Price (NYOP) business model can
   help soften competition. When consumers differ in their frictional costs
   (i.e., the shopping hassle) they experience when bidding at an NYOP
   retailer, the NYOP format can be a mechanism for differentiating a
   retailer from a posted-price rival. Beyond providing a motivation for
   using an NYOP mechanism, competition also has important implications for
   the optimal structure of the NYOP format. For example, this paper shows
   that prohibiting rebidding may benefit an NYOP firm by reducing price
   rivalry.}},
