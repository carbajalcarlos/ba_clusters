@inproceedings{ ISI:000174952600019,
Author = {Gordijn, J and Akkermans, H and Van Vliet, H},
Editor = {{Dieng, R and Corby, O}},
Title = {{What's in an electronic business model?}},
Booktitle = {{KNOWLEDGE ENGINEERING AND KNOWLEDGE MANAGEMENT, PROCEEDINGS: METHODS,
   MODELS, AND TOOLS}},
Series = {{LECTURE NOTES IN ARTIFICIAL INTELLIGENCE}},
Year = {{2000}},
