@incollection{ ISI:000341250600003,
Author = {Rex, John H. and Goldberger, Mark and Eisenstein, Barry I. and Harney,
   Carrie},
Editor = {{Bush, K}},
Title = {{The evolution of the regulatory framework for antibacterial agents}},
Booktitle = {{ANTIMICROBIAL THERAPEUTICS REVIEWS: INFECTIOUS DISEASES OF CURRENT AND
   EMERGING CONCERN}},
Series = {{Annals of the New York Academy of Sciences}},
Year = {{2014}},
Volume = {{1323}},
Pages = {{11-21}},
Abstract = {{The rising tide of antibacterial resistance and the lack of a diverse,
   vibrant pipeline of novel antibacterial agents is a global crisis that
   impairs our ability to treat life-threatening infections. The recent
   introduction of a tiered approach to the regulatory framework in this
   area offers one path to resolving some of the challenges. By drawing
   heavily on the predictive power of the related sciences of
   pharmacokinetics and pharmacodynamics, smaller, focused clinical trial
   programs have become possible for agents that might not otherwise have
   been possible to progress. There are limitations to these pathways, and
   they are not easy to implement, but making reliable noninferiority-based
   approaches available is critical to reinvigorating the global antibiotic
   pipeline. With the recognition of these ideas by key regulatory
   authorities in recent guidance, the next challenges in this area will
   focus on interpretive breakpoints, the extent of data in the prescribing
   information, ensuring that multiple agents can be progressed, and the
   challenge of the antibiotic business model.}},
