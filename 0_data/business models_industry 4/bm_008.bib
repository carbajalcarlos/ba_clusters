@article{ ISI:000328303000004,
Author = {Velu, Chander and Stiles, Philip},
Title = {{Managing Decision-Making and Cannibalization for Parallel Business
   Models}},
Journal = {{LONG RANGE PLANNING}},
Year = {{2013}},
Volume = {{46}},
Number = {{6, SI}},
Pages = {{443-458}},
Month = {{DEC}},
Abstract = {{This paper examines how a firm can manage the decision-making and
   cannibalization processes when a new and an existing business model need
   to be run in parallel. We present an in-depth longitudinal case study of
   a major bank in the US corporate bond trading market that launched a
   disruptive business model and ran it alongside its existing
   well-established and successful business model. The study shows how the
   firm conducting a staged decision making process that balanced
   procedural rationality and political expediency facilitates and helped
   resolve the paradoxes involved in running conflicting business models.
   We contribute to the decision making literature by showing how the
   mechanisms for balancing procedural rationality and politics facilitated
   the management of the decision-making and cannibalization processes and
