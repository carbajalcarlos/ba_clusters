@article{ ISI:000346170100010,
Author = {Wan, Jiafu and Zou, Caifeng and Zhou, Keliang and Lu, Rongshuang and Li,
   Di},
Title = {{IoT sensing framework with inter-cloud computing capability in vehicular
   networking}},
