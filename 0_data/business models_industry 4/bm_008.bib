@article{ ISI:000322424800002,
Author = {Al-Aali, Abdulrahman Y. and Teece, David J.},
Title = {{Towards the (Strategic) Management of Intellectual Property:
   RETROSPECTIVE AND PROSPECTIVE}},
Journal = {{CALIFORNIA MANAGEMENT REVIEW}},
Year = {{2013}},
Volume = {{55}},
Number = {{4, SI}},
Pages = {{15-30}},
Month = {{SUM}},
Abstract = {{The systematic integrated management of intellectual property (IP) is a
   recent phenomenon. This is despite the fact that intellectual property
   has been around for several centuries. Today, matters are more
   complicated, and integrated IP management is required. By integrated IP
   management we mean not only that the various forms of intellectual
   property (patents, trade secrets, trademarks, copyright) are managed
   together, but that intellectual property management is in turn
   integrated with overall business model design and corporate strategy.
   Integrated management is more than simply establishing a licensing
   model, or manufacturing a product that incorporates new invention.}},
