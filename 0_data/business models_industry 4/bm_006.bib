@article{ ISI:000370814400001,
Author = {Sioshansi, Fereidoon P.},
Title = {{Electricity utility business not as usual}},
Journal = {{ECONOMIC ANALYSIS AND POLICY}},
Year = {{2015}},
Volume = {{48}},
Pages = {{1-11}},
Month = {{DEC}},
Abstract = {{A number of the electric power industry's dimension are rapidly changing
   including generation, transmission, distribution and, most importantly,
   in relation to its customers. The fastest and most pronounced
   transformation is taking place in distributed energy resources or DERs -
   which include both energy efficiency improvements and distributed
   generation. The former allows consumers to use less; the latter allows
   them to generate more of what they need. Combined, they are turning an
   increasing number of consumers into prosumers, eroding utility revenues
   and threatening the historical business model, which has been based on
   fixed tariffs applied to volumetric consumption. This paper examines the
   ramifications of these developments and their implications for the power
   sector. (C) 2015 Economic Society of Australia, Queensland. Published by
   Elsevier B.V. All rights reserved.}},
