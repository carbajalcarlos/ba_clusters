Language = {{English}},
Affiliation = {{Shimura, H (Reprint Author), 7-3-1 Hongo,Bunkyo Ku, Tokyo 1130033, Japan.
   Shimura, H.; Masuda, S.; Kimura, H., Univ Tokyo, Grad Sch Pharmaceut Sci, Pharmacobusiness Innovat Lab, Tokyo, Japan.}},
DOI = {{10.1111/jcpt.12126}},
ISSN = {{0269-4727}},
EISSN = {{1365-2710}},
