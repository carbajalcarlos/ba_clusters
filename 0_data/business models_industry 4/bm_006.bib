@article{ ISI:000356879600053,
Author = {Cheng, Chin-Chi and Lee, Dasheng and Wang, Ching Hung and Lin, Shu Fen
   and Chang, Hung-Peng and Fang, Shang-Te},
Title = {{The Development of Cloud Energy Management}},
