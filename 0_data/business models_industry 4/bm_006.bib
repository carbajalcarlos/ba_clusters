@article{ ISI:000412174900002,
Author = {Bernt, Matthias and Colini, Laura and Foerste, Daniel},
Title = {{Privatization, Financialization and State Restructuring in Eastern
   Germany: The case of Am sudpark}},
Journal = {{INTERNATIONAL JOURNAL OF URBAN AND REGIONAL RESEARCH}},
