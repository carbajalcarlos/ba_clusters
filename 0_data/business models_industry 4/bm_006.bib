@incollection{ ISI:000288157000010,
Author = {Markowska, Magdalena and Saemundsson, Roegnvaldur J. and Wiklund, Johan},
Editor = {{Alsos, GA and Carter, S and Ljunggren, E and Welter, F}},
Title = {{Contextualizing business model development in Nordic rural gourmet
   restaurants}},
Booktitle = {{HANDBOOK OF RESEARCH ON ENTREPRENEURSHIP IN AGRICULTURE AND RURAL
   DEVELOPMENT}},
Year = {{2011}},
Pages = {{162-179}},
Abstract = {{Rural gourmet restaurants are becoming a vital part of regional
   development. The growing interest in high quality regional food, and the
   changing perception of values inherent in food, creates possibilities
   for many enterprising individuals. Through an in-depth case study of
   four Nordic rural gourmet restaurants, we explore the role of context in
   the process of business model development. By analysing the changes in
   the business model, we find that the development of the business
   reflects the interaction with the environment; in particular, that the
   more professionally embedded entrepreneurs are more active in searching
   their networks for ideas that might help them improve their business and
   they act more on contingencies, while restaurateurs with lesser
   professional embeddedness have exhibited less change in their business
   models and did not engage in extending their networks. Thus, we argue
   that understanding the context of how experiences as the main arena for
   value creation are created and sold is important for understanding how
   entrepreneurs can develop their local story and local identity and
   become co-producers of regional development.}},
