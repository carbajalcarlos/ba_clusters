@article{ ISI:000311545200002,
Author = {Martinez, Alberte},
Title = {{Energy Innovation and Transport: The Electrification of Trams in Spain,
   1896-1935}},
Journal = {{JOURNAL OF URBAN TECHNOLOGY}},
Year = {{2012}},
Volume = {{19}},
Number = {{3}},
Pages = {{3-24}},
Abstract = {{The electrification of trams was one of the most significant changes in
   urban transport. It led to price reductions, increased speed, better
   regularity, comfort and convenience, and the popularization of this
   means of transport. Its introduction required a new business model,
   characterized by massive investment, modern management, and the use of
   more sophisticated technology. This innovation came late to Spain and
   its arrival was aided by capital from Belgian investors and German
   multinational corporations.}},
