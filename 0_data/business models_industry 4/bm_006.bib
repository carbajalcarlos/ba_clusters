@article{ ISI:000312754400006,
Author = {Kyriakidou, Vagia and Katsianis, Dimitris and Orfanos, Iakovos and
   Chipouras, Aristeidis and Varoutas, Dimitris},
Title = {{Business modeling and financial analysis for Metropolitan Area Networks:
   Evidence from Greece}},
Journal = {{TELEMATICS AND INFORMATICS}},
Year = {{2011}},
Volume = {{28}},
Number = {{2}},
Pages = {{112-124}},
Month = {{MAY}},
Abstract = {{A sustainable business model is necessary for viable and future proof
   fiber-based Metropolitan Area Networks (MANs), in light of FITH/O
   developments, which necessitate public initiatives, towards the leverage
   of broadband adoption. Following a detailed analysis and benchmarking of
   international practices, a pool of business solutions for the MANs
   development, especially for the Greek case, is presented. Taking into
   account pros and cons, a set of four appropriate business models is
   analyzed, and a quantitative financial analysis concludes the findings.
   Guidelines for the further development of these initiatives, useful for
   other countries with low broadband penetration, are presented and
   discussed. (C) 2010 Elsevier Ltd. All rights reserved.}},
