@article{ ISI:000381440500002,
Author = {Stritar, Rok and Drnovsek, Mateja},
Title = {{What entrepreneurs discover when creating opportunities? Insights from
   Skype and YouTube ventures}},
Journal = {{INTERNATIONAL ENTREPRENEURSHIP AND MANAGEMENT JOURNAL}},
Year = {{2016}},
Volume = {{12}},
Number = {{3}},
Pages = {{659-679}},
Month = {{SEP}},
Abstract = {{Although the scholarly conversation about how entrepreneurial
   opportunities emerge has suggested that entrepreneurs both discover and
   create opportunity components, specific knowledge about what components
   are discovered is lacking. In this research, we use an exploratory case
   study to investigate the opportunity creation process. We found that
   entrepreneurs discover several opportunity-related components based on
   the prior experience and knowledge of other entrepreneurs. Drawing on
   the evidence from these exploratory cases, we identify three important
   types of components that entrepreneurs creatively recombine within an
   emerging opportunity: technology stack, business model, and product and
   service design architecture. These findings have important implications
   for our understanding of entrepreneurial bricolage and entrepreneurial
   recycling, and their connection to the process of opportunity creation.}},
