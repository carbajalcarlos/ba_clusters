Booktitle = {{ADVANCES IN MULTIMEDIA INFORMATION PROCESSING - PCM 2004, PT 2,
   PROCEEDINGS}},
Series = {{LECTURE NOTES IN COMPUTER SCIENCE}},
Year = {{2004}},
