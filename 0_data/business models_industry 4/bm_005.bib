@article{ ISI:000398861900001,
Author = {Laux, Christian and Rauter, Thomas},
Title = {{Procyclicality of US Bank Leverage}},
Journal = {{JOURNAL OF ACCOUNTING RESEARCH}},
Year = {{2017}},
Volume = {{55}},
Number = {{2}},
Pages = {{237-273}},
Month = {{MAY}},
Abstract = {{In light of the current debate about the link between accounting and
   financial stability, we investigate the determinants of procyclical book
   leverage for U.S. commercial and savings banks. We find that total asset
   growth and GDP growth are both positively related to book leverage
   growth. Our evidence is not consistent with the notion that fair value
   accounting contributes to procyclical leverage or that historical cost
   accounting reduces procyclicality. Overall, the business model of banks
   is more important for procyclical leverage than accounting or regulatory
   risk weights.}},
