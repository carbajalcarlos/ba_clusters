@article{ ISI:000396515800004,
Author = {Chen, Hong-Mei and Schuetz, Roland and Kazman, Rick and Matthes, Florian},
Title = {{How Lufthansa Capitalized on Big Data for Business Model Renovation}},
Journal = {{MIS QUARTERLY EXECUTIVE}},
Year = {{2017}},
Volume = {{16}},
Number = {{1}},
Pages = {{19-34}},
Month = {{MAR}},
Abstract = {{Enterprises have begun to derive value from big data, but many
   challenges remain. This article reports how Lufthansa successfully
   discovered big data value, addressed the technical complexities, and
   used big data as the basis for renovating its traditional business model
   to one that embraces customers as value co-creators. From Lufthansa's
   experience, we identify the challenges and critical success factors for
   innovating with big data and navigating through uncharted waters. The
   key is to shift the focus from technology to business values.}},
