@article{ ISI:000390950000007,
Author = {Kim, Hwa-Seon},
Title = {{A study on problem-solving writing instruction model with the use of big
   data}},
Journal = {{INTERNATIONAL JOURNAL OF KNOWLEDGE AND LEARNING}},
Year = {{2016}},
Volume = {{11}},
Number = {{2-3, SI}},
Pages = {{156-165}},
Abstract = {{The aim of this study is to investigate a creative writing teaching
   method for the college students' self-discovery with the use of big
   data. The following thesis proposes a way of utilising big data into
   problem-solving strategies for writing lesson plans. Nowadays, big data
   has great influence in diverse academic and industrial fields. In this
   paper, big data is the crucial medium that enhances critical and logical
   thinking in the writing process. With the use of big data, this study
   intends to introduce the writing strategies that lead positive reasoning
   of the writing subjects (authors). Similar to the situation that big
   data creates a new business model, big data will play a major role in
   the area of writing education by establishing an innovative and creative
   writing model. This study also tries to develop the college students'
   self-discovery through creative and specific writing education.}},
