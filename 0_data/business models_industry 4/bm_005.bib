@article{ ISI:000376063600003,
Author = {Khokhar, Rashid Hussain and Fung, Benjamin C. M. and Iqbal, Farkhund and
   Alhadidi, Dima and Bentahar, Jamal},
Title = {{Privacy-preserving data mashup model for trading person-specific
   information}},
Journal = {{ELECTRONIC COMMERCE RESEARCH AND APPLICATIONS}},
