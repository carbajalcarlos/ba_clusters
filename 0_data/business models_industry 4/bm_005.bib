
@article{ ISI:000384698000005,
Author = {Ramirez, Adriana G. and Tracci, Margaret C. and Stukenborg, George J.
   and Turrentine, Florence E. and Kozower, Benjamin D. and Jones, R. Scott},
Title = {{Physician-Owned Surgical Hospitals Outperform Other Hospitals in
   Medicare Value-Based Purchasing Program}},
Journal = {{JOURNAL OF THE AMERICAN COLLEGE OF SURGEONS}},
