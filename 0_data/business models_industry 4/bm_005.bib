@article{ ISI:000208824800008,
Author = {Salvatore, Dominick},
Title = {{The US challenge of European firms: globalisation, architecture,
   admiration and perceived innovativeness}},
Journal = {{EUROPEAN JOURNAL OF INTERNATIONAL MANAGEMENT}},
Year = {{2007}},
Volume = {{1}},
Number = {{1-2, SI}},
Pages = {{69-80}},
Abstract = {{During the past decade there has been a revolution in firm management,
   which greatly affected the relative international competitiveness of
   European, American, and Japanese firms. This paper examines this
