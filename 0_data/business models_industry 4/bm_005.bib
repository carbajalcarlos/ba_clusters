@article{ ISI:000225368700012,
Author = {Simon, AJ},
Title = {{A practical approach to the control and eradication of IBR using marker
   vaccines}},
Journal = {{CATTLE PRACTICE}},
Year = {{2004}},
Volume = {{12}},
Number = {{4}},
Pages = {{305-311}},
Month = {{OCT}},
Abstract = {{Due to the potential opening up of live export markets veterinary
   surgeons have an opportunity to be working with appropriate farmers to
   develop strategies to achieve and sustain BHV 1 free status to enable
   export of BHV 1 free cattle to countries which are BHV1 free or in the
   process of eradicating BHV1. In developing such a programme the
   individual circumstances of the farm should be taken into account - in
   particular the local geography, biosecurity measures and business model
   (which will affect the risk of re-infection), and also the long term
   commitment of the farmer as eradication from many herds will not be a
   short term strategy. The issue of latency with a-herpes viruses such as
   BHV 1 means that the use of vaccination to suppress shedding from
   latently infected cattle and to protect sero-negative cattle from such
   animals is an important part of any eradication scheme, this is
   particularly so where BHV1 is endemic with a high sero-prevalence as in
   the UK. In this situation there are obvious advantages to the use of
   marker vaccines on a herd basis to distinguish between cattle which have
   been vaccinated and those which have been infected with wild BHV1.}},
