Author = {Peterson, Jennie Hope and Chang, Jimmy and Wong, Yiu-Hing and Lawrence,
   Carl A.},
Editor = {{Cheng, TCE and Choi, TM}},
Title = {{The Emergence of the Fast Fashion Business Model and Imposed Quick
   Response Challenges for Chinese Fabric Manufacturers}},
Booktitle = {{INNOVATIVE QUICK RESPONSE PROGRAMS IN LOGISTICS AND SUPPLY CHAIN
   MANAGEMENT}},
