@article{ ISI:000428174800001,
Author = {Sukrat, Supattana and Papasratorn, Borworn},
Title = {{An architectural framework for developing a recommendation system to
   enhance vendors' capability in C2C social commerce}},
Journal = {{SOCIAL NETWORK ANALYSIS AND MINING}},
