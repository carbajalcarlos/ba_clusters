@article{ ISI:000419291500004,
Author = {Mrak, Marija Kastelan and Vukobrat, Nada Bodiroga and Sokolic, Danijela},
Title = {{PERSONALIZED MEDICINE INDUSTRY MODEL DEVELOPMENT}},
Journal = {{MANAGEMENT-JOURNAL OF CONTEMPORARY MANAGEMENT ISSUES}},
