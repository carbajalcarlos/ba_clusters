@article{ ISI:000412620600027,
Author = {Nucera, Federico and Lucas, Andre and Schaumburg, Julia and Schwaab,
   Bernd},
Title = {{Do negative interest rates make banks less safe?}},
Journal = {{ECONOMICS LETTERS}},
Year = {{2017}},
Volume = {{159}},
Pages = {{112-115}},
Month = {{OCT}},
Abstract = {{We study the impact of increasingly negative central bank policy rates
   on banks' propensity to become undercapitalized in a financial crisis
   ('SRisk'). We find that the risk impact of negative rates depends on
   banks' business models: Large banks with diversified income streams are
   perceived as less risky, while smaller and more traditional banks are
   perceived as more risky. Policy rate cuts below zero trigger different
   SRisk responses than an earlier cut to zero. (C) 2017 Elsevier B.V. All
   rights reserved.}},
