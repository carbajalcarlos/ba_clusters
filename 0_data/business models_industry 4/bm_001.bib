@article{ ISI:000425075400038,
Author = {Wang, Chih-Hsuan and Cheng, Hou-Yu and Deng, Yu-Ting},
Title = {{Using Bayesian belief network and time-series model to conduct
   prescriptive and predictive analytics for computer industries}},
