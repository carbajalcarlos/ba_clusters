@article{ ISI:000448468700009,
Author = {Hinings, C. R. (Bob)},
Title = {{Why should we bother? What are configurations for?}},
Journal = {{STRATEGIC ORGANIZATION}},
Year = {{2018}},
Volume = {{16}},
Number = {{4}},
Pages = {{499-509}},
Month = {{NOV}},
Abstract = {{The main thrust of the essays by Greckhamer et al., Tauscher, and Miller
   is about methodologies for studying configurations, such as qualitative
   comparative analysis, clustering, and systems dynamics. My aim is to ask
   why we should be interested in configurations at all. I consider three
   approaches to configurational analysis: first, configurations as a basic
   building block in a systematic theory of organizational structure and
   systems; second, configurations as a generic classification tool,
   emphasizing that all theory should be looking for enduring patterning in
   elements of all kinds associated with different organizational issues
   and different levels of analysis; third, the use of a configurational
   approach to consider the properties of individual cases. I also have
   some observations about the future of configurational analysis.}},
