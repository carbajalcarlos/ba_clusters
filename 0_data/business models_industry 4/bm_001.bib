@article{ ISI:000437263800006,
Author = {Hanafizadeh, Payam and Mehrabioun, Mohammad and Badie, Kambiz and Soofi,
   Jahanyar Bamdad},
Title = {{A Systemic Framework for Business Model Design and Development -Part A:
   Theorizing Perspective}},
Journal = {{SYSTEMIC PRACTICE AND ACTION RESEARCH}},
