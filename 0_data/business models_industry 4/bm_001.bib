@article{ ISI:000432126900001,
Author = {Biroscak, Brian J. and Schneider, Tali and Tyson, Dinorah Martinez and
   Loi, Claudia X. Aguado and Bryant, Carol A.},
Title = {{Applying Tools From Human-Centered Design to Social Marketing Planning}},
