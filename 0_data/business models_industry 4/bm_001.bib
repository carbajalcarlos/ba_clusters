@article{ ISI:000439864300002,
Author = {Aloshyna, Tetiana and Kozenkov, Dmytro},
Title = {{INNOVATION-DRIVEN GROWTH MODEL IN THE PRESENT CONTEXT OF BUSINESS
   PERFORMANCE}},
