@article{ ISI:000444824900013,
Author = {Xue, Xiao and Gao, Giagia and Wang, Shufang and Feng, Zhiyong},
Title = {{Service Bridge: Transboundary Impact Evaluation Method of Internet}},
Journal = {{IEEE TRANSACTIONS ON COMPUTATIONAL SOCIAL SYSTEMS}},
Year = {{2018}},
Volume = {{5}},
