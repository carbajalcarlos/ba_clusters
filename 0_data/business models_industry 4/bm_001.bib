@article{ ISI:000435146400004,
Author = {Allais, Romain and Gobert, Julie},
Title = {{Environmental assessment of PSS, feedback on 2 years of experimentation}},
Journal = {{MATERIAUX \& TECHNIQUES}},
Year = {{2018}},
Volume = {{105}},
Number = {{5-6}},
Month = {{MAY 30}},
Abstract = {{This communication details the sustainability assessment of the partial
   transition of business model from selling products to product renting
   for small household equipment (SHE). Perceived by the French SHE
   manufacturer as a strategic opportunity to meet customers' expectations
   and environmental regulation, 2-years experimentation was performed on a
   specific territory with the support of a network of new competencies
   (B-to-B-to-C market). Researchers were mandated for the sustainability
   assessment of such a transition but this communication focuses on the
   environmental performance of the experimentation. The results of the
   comparative LCA are presented and the main environmental impacts linked
   to this business model transition are specified and discussed. Then,
   different eco-design scenarios are explored and recommendations for this
   specific case are proposed.}},
