@article{ ISI:000441652400020,
Author = {Bissich, Peter and Wegmann, Kai and Lueitjens, Klaus and Gollnick,
   Volker},
Title = {{A hierarchical metamodeling approach for airline costs}},
Journal = {{JOURNAL OF AIR TRANSPORT MANAGEMENT}},
Year = {{2018}},
Volume = {{71}},
Pages = {{193-200}},
Month = {{AUG}},
Note = {{20th World Conference of the Air-Transport-Research-Society (ATRS),
   GREECE, JUN 23-26, 2016}},
Organization = {{Air Transport Res Soc}},
Abstract = {{Costs are a crucial basis for every decision process that combines
   resources to provide a product or service. Airlines decide on cost
   information about aircraft orders, route adaptions and business model
   changes. This paper shows a hierarchical metamodeling approach for
   airline total operation costs. In particular, we establish a hierarchy
   that supports the decision-making processes to address any
   operational-driven impact to the air transportation system. First use
   case is the business model of the long-haul low-cost carrier. The model
   currently overestimates the total operating costs. The reasons for the
   overestimation are the varying cost items of different airline business
   types and world regions as well as the lack of an appropriate
   activity-based cost model for the airline sector to assign overhead
   costs.}},
