@article{ ISI:000437118800006,
Author = {Alserhan, Baker Ahmad and Wood, Bronwyn P. and Rutter, Richard and
   Halkias, Daphne and Terzi, Hasan and Al Serhan, Omar},
Title = {{The transparency of Islamic hotels: ``Nice Islam{''} and the
   ``self-orientalizing{''} of Muslims?}},
