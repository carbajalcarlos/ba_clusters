@article{ ISI:000411492100002,
Author = {Widjojo, Francisco},
Title = {{Microfinance in Australia: Is the Law Doing Enough to Address Financial
   Exclusion?}},
Journal = {{JOURNAL OF BANKING AND FINANCE LAW AND PRACTICE}},
Year = {{2017}},
Volume = {{28}},
Number = {{3}},
Pages = {{195-220}},
Month = {{SEP}},
Abstract = {{``Microfinance{''} has been used as a means to address financial
   exclusion in Australia since 2003. However, several factors are
   inhibiting the success of microfinance in Australia. First, in its
   current form, microfinance does not adequately address all the root
   causes of financial exclusion. Secondly, microfinance institutions in
   Australia do not operate under a financially sustainable business model.
   This article critically analyses the current legal framework
   underpinning microfinance in Australia and proposes a significant
   regulatory solution in the form of a mandatory referral regime to
   rectify the first limitation based on an approach being used in the UK
   banking sector. Ultimately, it is concluded that it is essential for the
   Australian government to intervene through law to better support
   microfinance and that, if it does not do so, it risks financial
   exclusion becoming more prevalent within Australian society.}},
