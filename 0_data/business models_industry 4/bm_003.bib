@article{ ISI:000416146800003,
Author = {Kaulio, Matti and Thoren, Kent and Rohrbeck, Rene},
Title = {{Double ambidexterity: How a Telco incumbent used business-model and
   technology innovations to successfully respond to three major
   disruptions}},
Journal = {{CREATIVITY AND INNOVATION MANAGEMENT}},
Year = {{2017}},
Volume = {{26}},
Number = {{4}},
Pages = {{339-352}},
Month = {{DEC}},
Abstract = {{We leverage the business model innovation and ambidexterity literature
   to investigate a contradictory case, the Swedish-Finnish Telecom
   operator TeliaSonera. Despite being challenged by three major
   disruptions, the company not only still exists but also enjoys
   remarkably good financial performance. Building on extant archival data
   and interviews, we carefully identify and map 26 organizational
   responses during 1992-2016. We find that the firm has overcome three
   critical phases by experimenting and pioneering with portfolios of
   business models and/or technological innovations. We describe this
   behaviour as double ambidexterity. We use an in-depth case study to
   conceptualize double ambidexterity and discuss its impact on the
   business's survival and enduring success.}},
