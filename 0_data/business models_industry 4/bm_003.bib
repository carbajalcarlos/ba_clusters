@article{ ISI:000403232300017,
Author = {Rao, Krishna C. and Otoo, Miriam and Drechsel, Pay and Hanjra, Munir A.},
Title = {{Resource Recovery and Reuse as an Incentive for a More Viable Sanitation
   Service Chain}},
