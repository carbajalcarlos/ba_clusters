@incollection{ ISI:000357026500007,
Author = {Meinhold, Joachim},
Editor = {{Nienstedt, HW and RussMohl, S and Wilczek, B}},
Title = {{Resisting crisis rhetoric in financing journalism A managerial and
   customer oriented standpoint}},
Booktitle = {{JOURNALISM AND MEDIA CONVERGENCE}},
Series = {{Media Convergence}},
Year = {{2013}},
Volume = {{5}},
Pages = {{79-90}},
Abstract = {{At least when it comes to the German market we argue that the ``crisis
   rhetoric{''} about newspapers is highly questionable. The traditional
   financing model for journalistic content, enhanced by new but related
   businesses, still enables a return on sales well into double-digit
   figures as it is the case in well managed newspaper publishing houses.
   The model does indeed require a critical discussion on journalistic and
   publishing optimization in terms of strategic perspective. In this
   context we discuss ten issues which should be approached to optimize the
   newspaper business. Alternative financing models like non-profit
   organizations and cooperatives on the other hand are seen with
   scepticism. They especially bear dangers concerning the professional
   governance of publishing houses and the preservation of editorial
   independence.}},
