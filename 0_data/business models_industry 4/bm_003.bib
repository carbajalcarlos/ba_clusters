@article{ ISI:000444470800002,
Author = {Lee, Hau L.},
Title = {{Big Data and the Innovation Cycle}},
Journal = {{PRODUCTION AND OPERATIONS MANAGEMENT}},
Year = {{2018}},
Volume = {{27}},
Number = {{9, SI}},
Pages = {{1642-1646}},
Month = {{SEP}},
Abstract = {{Big data and the related methodologies could have great impacts on
   operations and supply chain management. Such impacts could be realized
   through innovations leveraging big data. The innovations can be
   described as first improving existing processes in operations through
   better tools and methods; second expanding the value propositions
   through expansive usage or incorporating data not available in the past;
   and third allowing companies to create new processes or business models
   to serve customers in new ways. This study describes this framework of
   the innovation cycle.}},
Publisher = {{WILEY}},
Address = {{111 RIVER ST, HOBOKEN 07030-5774, NJ USA}},
Type = {{Article}},
