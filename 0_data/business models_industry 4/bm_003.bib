@article{ ISI:000270873900003,
Author = {Polk, Charles},
Title = {{MARS BY WAY OF STORYTELLERS AND PATRONS}},
Journal = {{JBIS-JOURNAL OF THE BRITISH INTERPLANETARY SOCIETY}},
Year = {{2009}},
Volume = {{62}},
Number = {{5}},
Pages = {{187-197}},
Month = {{MAY}},
Abstract = {{A business model is presented in which space-themed entertainment and
   educational media are harnessed to the goal of designing and financing
   the establishment of a self-sustaining Mars outpost. The model draws
   from two precedents: (i) The National Geographic Society's use of media
   sales to fund research and (ii) the historical role of patrons in the
   process of creativity. These precedents are combined in an international
   non-governmental organization (INGO) that is governed by the individuals
   who participate in the media businesses branded by the INGO - these
   individuals constitute a patron base of several tens of millions.
   Requiring only modest penetration into global media markets,
   expenditures of USD 5 billion per year (in 2008 dollars) can commence in
   2025 and continue at an accelerating rate until the Mars outpost is
   established. The financing approach utilized, combined with a decision
   authority that is free to consider a broad range of solutions, will
   advance the technical state-of-the-art and result in an Earth-Mars
   infrastructure capable of expanding the initial Mars Outpost into
   humanity's first second home.}},
