@article{ ISI:000224890700004,
Author = {Choi, JS},
Title = {{Spatial analysis of transactions using e-catalogs in public
   business-to-business electronic marketplaces by business model in Korea}},
Journal = {{URBAN GEOGRAPHY}},
Year = {{2004}},
Volume = {{25}},
Number = {{4}},
Pages = {{332-352}},
Month = {{MAY 16}},
Abstract = {{The objective of this study is to analyze spatial flows of products
   traded over public business-to-business electronic marketplaces (B2B
   e-MPs) using a business model. The transaction data for six months over
   an e-MP dealing with maintenance, repair, and operating (MRO) products
   were acquired, and the spatial flow of the products traded over the e-MP
   was analyzed based on two business models: an exchange model and an
   agent model. The analysis showed a differentiated spatial impact by the
   business model. Customers with the exchange model strongly preferred
   intraregional suppliers. However, those following the agent model tended
   to be supplied from interregional suppliers. Although the items involved
   with the agent model were mainly the standardized operating inputs, the
   high ratio of interregional suppliers in the agent model were revealed
   irrespective of the types of products traded.}},
