@inproceedings{ ISI:000170923500016,
Author = {Briscoe, B},
Editor = {{Rizzo, L and Fdida, S}},
Title = {{The direction of value flow in connectionless networks}},
Booktitle = {{NETWORKED GROUP COMMUNICATION, PROCEEDINGS}},
