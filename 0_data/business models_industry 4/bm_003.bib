@article{ ISI:000395831200009,
Author = {Tan, Jun and Wang, Lingfeng},
Title = {{Adequacy Assessment of Power Distribution Network With Large Fleets of
   PHEVs Considering Condition-Dependent Transformer Faults}},
Journal = {{IEEE TRANSACTIONS ON SMART GRID}},
Year = {{2017}},
Volume = {{8}},
Number = {{2}},
Pages = {{598-608}},
Month = {{MAR}},
Abstract = {{As a new form of distributed energy resources, massive plug-in hybrid
   electric vehicles (PHEVs) could affect the power distribution system
   adequacy considering their intermittent charging loads and the load
   recovery ability during system outages. This paper proposes a
   comprehensive framework for adequacy evaluation of power distribution
   networks with PHEVs penetration. A condition-dependent outage model is
   used in this paper to obtain the time sequential failure rate of the
   transformer. Also, a business model for the PHEVs is developed to
   encourage the PHEV owners to charge their vehicles in such a way that
   the distribution system adequacy is enhanced. Based on this model, a
