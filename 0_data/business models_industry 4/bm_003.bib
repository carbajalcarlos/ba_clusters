@article{ ISI:000300547400014,
Author = {Marlow, David R. and Moglia, Magnus and Beale, David J. and Stenstromer,
   Anna},
Title = {{Embedding sustainability into a utility's business culture}},
Journal = {{JOURNAL AMERICAN WATER WORKS ASSOCIATION}},
Year = {{2012}},
Volume = {{104}},
Number = {{2}},
Pages = {{51-52}},
Month = {{FEB}},
Abstract = {{Engagement with sustainability can be viewed as a process in which
   initiatives involving management, employees, stakeholders, and the
   community gradually facilitate changes in direction and attitude that
   are in line with sustainability principles. This view holds that an
   organization's culture is a key point of leverage in any attempt to move
   to a business model more closely aligned with sustainability concepts.
   With this in mind, an approach was developed to analyze business
   decisions in order to assess the sustainability culture of a water
   utility. This article presents an overview of this research, including
   background on how an organization's culture can influence decisions. The
   methodology for analyzing decision making from a sustainability policy
   perspective is demonstrated using a case study undertaken with an
   Australian public water utility.}},
