@article{ ISI:000415723000019,
Author = {Gnoni, Maria Grazia and Mossa, Giorgio and Mummolo, Giovanni and
   Tornese, Fabiana and Verriello, Rossella},
Title = {{CIRCULAR ECONOMY STRATEGIES FOR ELECTRIC AND ELECTRONIC EQUIPMENT: A
   FUZZY COGNITIVE MAP}},
