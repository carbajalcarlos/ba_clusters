@article{ ISI:000428816900005,
Author = {Qiu, Xuan and Wong, Eugene Y. C. and Lam, Jasmine Siu Lee},
Title = {{Evaluating economic and environmental value of liner vessel sharing
   along the maritime silk road}},
Journal = {{MARITIME POLICY \& MANAGEMENT}},
Year = {{2018}},
Volume = {{45}},
Number = {{3}},
Pages = {{336-350}},
