@article{ ISI:000424584200002,
Author = {Augustyniak, Dobrochna},
Title = {{Sources of Value Creation in Born Global Companies}},
Journal = {{INTERNATIONAL JOURNAL OF MANAGEMENT AND ECONOMICS}},
Year = {{2017}},
Volume = {{53}},
Number = {{2}},
Pages = {{7-22}},
Month = {{JUN}},
Abstract = {{Research about the phenomenon of rapid internationalization of small and
   medium size companies has evolved over the last two decades.
   Nevertheless, questions about business models or value creation in born
   global enterprises are rarely addressed in the literature. The objective
   of this article is to develop a theoretical framework for analyzing the
   sources of value creation in the business models of international new
   ventures (INV) global companies, starting with adapting the model by
   Amit and Zott {[}2001, 2010] to early internationalizing venture.
