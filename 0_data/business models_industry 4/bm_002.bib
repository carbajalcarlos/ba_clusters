@article{ ISI:000214327300009,
Author = {Hassan, R. and Marwan, A. and Tawil, N. M. and Ramlee, S.},
Title = {{COOPERATIVE BIO BUSINESS MODEL: A STUDY AT UNIVERSITI KEBANGSAAN
   MALAYSIA}},
Journal = {{JOURNAL OF ENGINEERING SCIENCE AND TECHNOLOGY}},
Year = {{2015}},
Volume = {{10}},
Number = {{SI}},
Pages = {{81-88}},
Month = {{JUN}},
Abstract = {{Bio-economy or economy based on biology is soaring high in the reality
   scenario of the global business and grabbing attention of the
   policymakers all over the world. In order to herald the acceleration in
   the field of biodiversity and explore the immensely promising potentials
