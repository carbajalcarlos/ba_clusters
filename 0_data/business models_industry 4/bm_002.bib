@article{ ISI:000433956600002,
Author = {Muetterlein, Joschka and Kunz, Reinhard E.},
Title = {{Innovate alone or with others? Influence of entrepreneurial orientation
   and alliance orientation on media business model innovation}},
Journal = {{JOURNAL OF MEDIA BUSINESS STUDIES}},
Year = {{2017}},
Volume = {{14}},
Number = {{3, SI}},
Pages = {{173-187}},
Abstract = {{Being able to innovate one's business model has been a topic of
   paramount importance in media management research and practice. Recent
   studies suggest that the strategic orientation of a company is a central
   driver of business model innovation. However, empirical evidence is
   scarce. To fill this gap, we assess the impact of entrepreneurial and
   alliance orientation as two highly relevant strategic orientations with
   a quantitative survey of 50 companies. Our results show that a media
   company's entrepreneurial orientation has positive effects on its
   ability to innovate value creation and value proposition, but not on its
   ability to capture that value. In addition, we could not find
   significant results indicating that being alliance oriented is
   beneficial for business model innovation.}},
Publisher = {{ROUTLEDGE JOURNALS, TAYLOR \& FRANCIS LTD}},
Address = {{2-4 PARK SQUARE, MILTON PARK, ABINGDON OX14 4RN, OXON, ENGLAND}},
Type = {{Article}},
