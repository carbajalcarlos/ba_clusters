@article{ ISI:000368651300056,
Author = {Kim, Hyo-Yong and Yang, Sung-Bong and Lim, Soon-Bum},
Title = {{Interactive contents composition in media art of multi-display
   environment}},
Journal = {{ASIA LIFE SCIENCES}},
Year = {{2015}},
Number = {{12}},
Pages = {{725-736}},
Month = {{DEC}},
Abstract = {{Media art in multi-display environment can provide an experiential
   setting that allows the audience to actively participate through its
   highly engaging interactive content and storytelling. This requires
   research in technology development for organic synchronization among
   various types of devices including multi-displays and for cognitive
   features to allow for mutual interactivity, while requiring studies on
   how to organize content on such displays. The above-mentioned
   technologies and organization methods can be seen as the most necessary
