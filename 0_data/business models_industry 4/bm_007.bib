@article{ ISI:000316737700003,
Author = {Stroetmann, Karl A.},
Title = {{Achieving the integrated and smart health and wellbeing paradigm: A call
   for policy research and action on governance and business models}},
Journal = {{INTERNATIONAL JOURNAL OF MEDICAL INFORMATICS}},
