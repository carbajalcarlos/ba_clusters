@article{ ISI:000340949000001,
Author = {Walters, William H.},
Title = {{E-books in academic libraries: Challenges for sharing and use}},
Journal = {{JOURNAL OF LIBRARIANSHIP AND INFORMATION SCIENCE}},
Year = {{2014}},
Volume = {{46}},
Number = {{2}},
Pages = {{85-95}},
Month = {{JUN}},
Abstract = {{This paper reviews the recent literature on e-book sharing and use in
   post-secondary libraries, exploring current restrictions on viewing,
   printing, downloading, circulation, and interlibrary loan. It also
   discusses the ways in which these restrictions influence the library
   lending of e-book readers and other mobile devices. The most fundamental
