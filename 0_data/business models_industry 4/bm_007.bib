@article{ ISI:000309735500002,
Author = {Levitin, Adam J.},
Title = {{BANKRUPT POLITICS AND THE POLITICS OF BANKRUPTCY}},
Journal = {{CORNELL LAW REVIEW}},
Year = {{2012}},
