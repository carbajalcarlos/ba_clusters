@article{ ISI:000087275200003,
Author = {Lang, KR and Zhao, JL},
Title = {{The role of electronic commerce in the transformation of distance
   education}},
Journal = {{JOURNAL OF ORGANIZATIONAL COMPUTING AND ELECTRONIC COMMERCE}},
