@article{ ISI:000341762100004,
Author = {Luis Requejo-Aleman, Jose and Lugo-Ocando, Jairo},
Title = {{ASSESSING THE SUSTAINABILITY OF LATIN AMERICAN INVESTIGATIVE NON-PROFIT
   JOURNALISM}},
