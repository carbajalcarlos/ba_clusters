@article{ ISI:000363351500020,
Author = {Nordtomme, Marianne Elvsaas and Bjerkan, Kristin Ystmark and Sund,
   Astrid Bjorgen},
Title = {{Barriers to urban freight policy implementation: The case of urban
   consolidation center in Oslo}},
Journal = {{TRANSPORT POLICY}},
