@article{ ISI:000441334300248,
Author = {Alonso, Alvaro and Pozo, Alejandro and Cantera, Jose Manuel and de la
   Vega, Francisco and Hierro, Juan Jose},
Title = {{Industrial Data Space Architecture Implementation Using FIWARE}},
