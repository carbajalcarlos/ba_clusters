@article{ ISI:000385369100011,
Author = {Veza, Ivica and Mladineo, Marko and Gjeldum, Nikola},
Title = {{SELECTION OF THE BASIC LEAN TOOLS FOR DEVELOPMENT OF CROATIAN MODEL OF
   INNOVATIVE SMART ENTERPRISE}},
