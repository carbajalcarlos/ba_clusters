@article{ ISI:000437997500050,
Author = {Ge, Mouzhi and Bangui, Hind and Buhnova, Barbora},
Title = {{Big Data for Internet of Things: A Survey}},
Journal = {{FUTURE GENERATION COMPUTER SYSTEMS-THE INTERNATIONAL JOURNAL OF ESCIENCE}},
Year = {{2018}},
Volume = {{87}},
Pages = {{601-614}},
Month = {{OCT}},
Abstract = {{With the rapid development of the Internet of Things (loT), Big Data
   technologies have emerged as a critical data analytics tool to bring the
   knowledge within loT infrastructures to better meet the purpose of the
   loT systems and support critical decision making. Although the topic of
   Big Data analytics itself is extensively researched, the disparity
   between loT domains (such as healthcare, energy, transportation and
   others) has isolated the evolution of Big Data approaches in each loT
   domain. Thus, the mutual understanding across loT domains can possibly
   advance the evolution of Big Data research in loT.
   In this work, we therefore conduct a survey on Big Data technologies in
   different loT domains to facilitate and stimulate knowledge sharing
   across the loT domains. Based on our review, this paper discusses the
   similarities and differences among Big Data technologies used in
   different loT domains, suggests how certain Big Data technology used in
   one loT domain can be re-used in another loT domain, and develops a
   conceptual framework to outline the critical Big Data technologies
   across all the reviewed loT domains. (C) 2018 Elsevier B.V. All rights
   reserved.}},
