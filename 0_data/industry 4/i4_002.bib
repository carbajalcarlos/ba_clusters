@article{ ISI:000441334300220,
Author = {Blanco-Novoa, Oscar and Fernandez-Carames, Tiago M. and Fraga-Lamas,
   Paula and Castedo, Luis},
Title = {{A Cost-Effective IoT System for Monitoring Indoor Radon Gas
   Concentration}},
