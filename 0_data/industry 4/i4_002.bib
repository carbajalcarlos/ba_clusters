@article{ ISI:000409808600004,
Author = {Branger, Jakob and Pang, Zhibo},
Title = {{From automated home to sustainable, healthy and manufacturing home: a
   new story enabled by the Internet- of- Things and Industry 4.0}},
Journal = {{JOURNAL OF MANAGEMENT ANALYTICS}},
Year = {{2015}},
Volume = {{2}},
Number = {{4}},
Pages = {{314-332}},
Abstract = {{This paper seeks to expand on the role for future homes in relation to
   automation, sustainability, healthcare and manufacturing. The world is
   changing and the home along with it. Services provided in the home will
