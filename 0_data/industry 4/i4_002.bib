@article{ ISI:000396397900003,
Author = {Ascorti, Leonardo and Savazzi, Stefano and Soatti, Gloria and Nicoli,
   Monica and Sisinni, Emiliano and Galimberti, Stefano},
Title = {{A Wireless Cloud Network Platform for Industrial Process Automation:
   Critical Data Publishing and Distributed Sensing}},
