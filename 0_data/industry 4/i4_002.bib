@article{ ISI:000444739000014,
Author = {Jabbar, Sohail and Khan, Murad and Silva, Bhagya Nathali and Han, Kijun},
Title = {{A REST-based industrial web of things' framework for smart warehousing}},
Journal = {{JOURNAL OF SUPERCOMPUTING}},
