Title = {{MATCHING RENEWABLE ENERGY PRODUCTION AND CONSUMPTION BY MARKET REGULATED
   DEMAND SITE MANAGEMENT (DSM)}},
Booktitle = {{EXPANDING BOUNDARIES: SYSTEMS THINKING IN THE BUILT ENVIRONMENT}},
Year = {{2016}},
